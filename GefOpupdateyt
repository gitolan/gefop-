    TeleportMenu.Visible = not TeleportMenu.Visible
    EspMenu.Visible = false
    PlayerMenu.Visible = false
    OtherMenu.Visible = false
    Teleport2Menu.Visible = false
    Player2Menu.Visible = false
end)

