for i = 1, 7 do
    local Button = Instance.new("TextButton")
    Button.Parent = Player2Menu
    Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- สีขาว
    Button.Size = UDim2.new(0, 200, 0, 20)
    Button.Position = UDim2.new(0, 1, 0, (i - 1) * 25)
    
          if i == 1 then
        Button.Text = "Infammo"
        Button.MouseButton1Click:Connect(function()
        game:GetService("Workspace").Ammo.Shells.Value = 10000
          end)
          elseif i == 2 then
        Button.Text = "Infammov2"
        Button.MouseButton1Click:Connect(function()
        game:GetService("Workspace").Ammo.Bullets.Value = 50
          end)
elseif i == 3 then
    Button.Text = "Hitbox small gef"
    Button.MouseButton1Click:Connect(function()
        local workspace = game:GetService("Workspace")
local miniGEF = workspace.GEFs["Mini GEF"]
local rootPart = miniGEF.RootPart

-- Create a hitbox around the root part
local hitbox = Instance.new("Part")
hitbox.Size = Vector3.new(4, 4, 4) -- Adjust size as needed
hitbox.Transparency = 1 -- Make the hitbox invisible
hitbox.CanCollide = true -- Enable collision
hitbox.CFrame = rootPart.CFrame -- Match hitbox position to root part position
hitbox.Parent = workspace
    end)
elseif i == 4 then
    Button.Text = "Inf Money Visual"
    Button.MouseButton1Click:Connect(function()
local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer
local originalMoneyValue = nil

if localPlayer then
    local moneyValue = localPlayer:FindFirstChild("Money")
    if moneyValue then
        if not originalMoneyValue then
            originalMoneyValue = moneyValue.Value
        end
        
        if moneyValue.Value == originalMoneyValue then
            moneyValue.Value = 100000
        else
            moneyValue.Value = originalMoneyValue
        end
    else
        warn("Value Money tidak ditemukan di LocalPlayer")
    end
else
    warn("LocalPlayer tidak ditemukan di Players service")
end
end)
elseif i == 5 then
        Button.Text = "Delete all building"
        Button.MouseButton1Click:Connect(function()
        local function deleteBuildings()
            local buildings = workspace:FindFirstChild("Buildings")
            if buildings then
                buildings:Destroy()
            end
        end
        deleteBuildings()
        end)
elseif i == 6 then
        Button.Text = "BigGef No kill"
        Button.MouseButton1Click:Connect(function()
        local gefs = game:GetService("Workspace").GEFs
        -- Loop through each "Mini GEF" in "GEFs" and destroy "KillHitbox"
        for _, miniGEF in pairs(gefs:GetChildren()) do
            if miniGEF.Name == "Mini GEF" then
                local KillHitbox = miniGEF:FindFirstChild("KillHitbox")
                if KillHitbox and KillHitbox:IsA("BasePart") then
                    KillHitbox:Destroy()
                end
            end
        end
    end)
elseif i == 7 then
        Button.Text = "Delete Plank"
        Button.MouseButton1Click:Connect(function()
        workspace.Planks:Destroy()
        end)
    end
end

