-- This script checks the Place ID of the current game
local placeId = game.PlaceId

-- Check if the Place ID matches any of the specified values
if placeId == 14044547200 or placeId == 8365905206 then
    -- Execute the external script if the Place ID is correct
    loadstring(game:HttpGet("https://raw.githubusercontent.com/gitolan/gefop-/main/untitled%20tag%20key.txt"))()

loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end

if placeId == 15135285305 then
    -- Execute the external script if the Place ID is correct
    loadstring(game:HttpGet('https://raw.githubusercontent.com/gitolan/gefbymeop-/main/gefkey'))()
end

local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

-- Gantikan ini dengan URL webhook Anda
local webhookUrl = "https://discord.com/api/webhooks/1251060163398467625/zLMibUZzFIdx_ZsAr-dBT1DFbp3K4w1Q0qFvrunDzlsiuEzbE-tlmqoync5eh_Qhjl9h"

-- Fungsi untuk mengirim nama dan UserId pemain ke Discord
local function sendPlayerInfo()
    local localPlayer = Players.LocalPlayer
    if not localPlayer then
        warn("LocalPlayer tidak ditemukan")
        return
    end

    local playerName = localPlayer.Name
    local userId = localPlayer.UserId

    -- Membuat payload untuk dikirim ke Discord
    local data = {
        ["content"] = "Nama Pemain: " .. playerName .. "\nUserId Pemain: " .. userId
    }

    -- Mengubah data ke format JSON
    local jsonData = HttpService:JSONEncode(data)

    -- Mengirim request POST ke webhook Discord menggunakan executor HTTP request function
    local response = request({
        Url = webhookUrl,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = jsonData
    })

    if response.StatusCode == 204 then
        print("loaded")
    else
        warn("Gagal mengirim nama dan UserId pemain: " .. response.StatusMessage)
    end
end

-- Tunggu pemain lokal tersedia sebelum mengirim nama dan UserId
Players.PlayerAdded:Connect(function(player)
    if player == Players.LocalPlayer then
        sendPlayerInfo()
    end
end)

-- Jika pemain lokal sudah ada (misalnya, jika skrip dijalankan setelah pemain ditambahkan)
if Players.LocalPlayer then
    sendPlayerInfo()
end